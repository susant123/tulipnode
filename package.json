  ],
  "name": "tulind-maintained",
  "version": "1.0.1",
  "description": "Maintained fork of Tulip Indicators - Over 100 Technical Analysis Functions with modern Node.js support",
  "main": "index.js",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/susant123/tulipnode"
  },
  "homepage": "https://github.com/susant123/tulipnode#readme",
  "bugs": {
    "url": "https://github.com/susant123/tulipnode/issues"
  },
  "keywords": [
    "tulip",
    "tulind",
    "tulipnode",
    "maintained",
    "fork",
    "charts",
    "charting",
    "indicators",
    "oscillator",
    "overlay",
    "technical",
    "analysis",
    "finance",
    "trading",
    "markets",
    "stocks",
    "crypto",
    "forex",
    "technical-analysis",
    "trading-indicators"
  ],
  "license": "LGPL-3.0",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "bindings": "^1.5.0",
    "minctest": "0.0.x",
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "aws-sdk": "2.x"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node test.js"
  },
  "binary": {
    "module_name": "tulind",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://tulipnode.s3.amazonaws.com"
